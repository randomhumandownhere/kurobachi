{
  "name": "vite-react-tailwindcss-browser-extension",
  "version": "1.0.0",
  "description": "A Cross Browser Extension template",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build:background:chrome": "vite build --config vite.chrome.config.ts",
    "build:popup": "vite build --config vite.popup.config.ts",
    "build:web": "tsc && vite build",
    "build": "NODE_ENV=production run-s build:web",
    "build:chrome": "NODE_ENV=production run-s build:background:chrome build:web",
    "package": "zip -r extension.zip dist/*",
    "preview": "vite preview"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/chrome": "^0.1.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/webextension-polyfill": "^0.9.1",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "chart.js": "^4.5.0",
    "formik": "^2.4.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0"
  }
}
